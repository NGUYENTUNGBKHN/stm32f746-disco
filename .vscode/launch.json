{
    "version": "0.2.0",
    "configurations": [
        {   // wp1/00-helloWorld
            "name": "wp1/00-helloWorld",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/normal/make_dev/wp1/00-helloWorld/_build/helloworld.out", // <--- IMPORTANT: Update this path to your compiled .elf file
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "device": "STM32F746NG", // <--- IMPORTANT: Adjust for your specific F746 part (e.g., STM32F746VG, STM32F746NG, etc.)
            "svdFile": "${workspaceFolder}/svd/STM32F746x.svd", // <--- IMPORTANT: Path to your SVD file
